{
  "name": "@myerscarpenter/quest-dev",
  "version": "1.0.1",
  "description": "CLI for Meta Quest Browser development - screenshot and URL opening via ADB and cdp-cli",
  "type": "module",
  "bin": {
    "quest-dev": "./build/index.js"
  },
  "main": "./build/index.js",
  "scripts": {
    "clean": "rm -rf build",
    "build": "pnpm run clean && tsc && chmod +x build/index.js",
    "start": "pnpm run build && node build/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "version:patch": "pnpm version patch",
    "version:minor": "pnpm version minor",
    "version:major": "pnpm version major"
  },
  "keywords": [
    "meta",
    "quest",
    "quest-browser",
    "adb",
    "cli",
    "automation",
    "vr",
    "xr",
    "screenshot"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/myers/quest-dev"
  },
  "dependencies": {
    "which": "^4.0.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "@types/which": "^3.0.4",
    "@types/yargs": "^17.0.33",
    "@vitest/ui": "^4.0.3",
    "c8": "^10.1.3",
    "typescript": "^5.9.2",
    "vitest": "^4.0.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@myerscarpenter/cdp-cli": ">=2.0.0"
  },
  "peerDependenciesMeta": {
    "@myerscarpenter/cdp-cli": {
      "optional": true
    }
  }
}
